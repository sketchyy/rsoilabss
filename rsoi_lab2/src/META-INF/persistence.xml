<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="rsoi2unit">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>com.sketchyy.rsoi2.entities.User</class>
        <class>com.sketchyy.rsoi2.entities.Code</class>
        <class>com.sketchyy.rsoi2.entities.Token</class>
        <class>com.sketchyy.rsoi2.entities.App</class>
        <class>com.sketchyy.rsoi2.entities.Allow</class>
        <class>com.sketchyy.rsoi2.entities.Actor</class>
        <class>Movie</class>
        <properties>
            <property name="javax.persistence.jdbc.url" value="jdbc:oracle:thin:@//localhost:1521/xe"/>
            <property name="javax.persistence.jdbc.driver" value="oracle.jdbc.OracleDriver"/>
            <property name="javax.persistence.jdbc.user" value="sketchyy"/>
            <property name="javax.persistence.jdbc.password" value="1234"/>
        </properties>
    </persistence-unit>
</persistence>
